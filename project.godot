config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Alien",
"language": "GDScript",
"path": "res://src/Alien.gd"
} ]
_global_script_class_icons={
"Alien": ""
}

[application]

config/name="SpaceInvader"
